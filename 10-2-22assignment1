#include<stdio.h>  
int main()
{  
int number, *p;
printf("Enter any number: "); 
scanf("%d",&number);     
p=&number; 
printf("before increment: \n");       
printf("Address of p variable is %u \n",p);        
p=p+1;        
printf("After increment: \n");
printf("Address of p variable is %u \n",p);      
return 0;  
}
